<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- General -->
    <skip />
    <!-- Application name -->
    <string name="application_name">Tvorba Giest</string>
    <!-- Title, name of the activity used to create a gesture -->
    <string name="label_create_gesture">Vytvoriť Gesto</string>

    <!-- Buttons -->
    <skip />
    <!-- Label, button used to add a gesture -->
    <string name="button_add">Pridať gesto</string>
    <!-- Label, button used to reload all gestures -->
    <string name="button_reload">Obnoviť</string>
    <!-- Label, button used to cancel the operation of adding a gesture -->
    <string name="button_discard">Zrušiť</string>
    <!-- Label, button used to save a gesture newly created -->
    <string name="button_done">Hotovo</string>
    
    <!-- Gestures -->
    <skip />
    <!-- Label, prompt asking the user to enter the name of the gesture -->
    <string name="prompt_gesture_name">Názov</string>
    <!-- Error message, informs the user he needs to enter a name before saving a gesture -->
    <string name="error_missing_name">Musíte zadať názov</string>
    <!-- success message, tells the user where the gesture was saved -->
    <string name="save_success">Gesto uložené v %s</string>
    <!-- Buttons in Rename gesture dialog box -->
    <string name="rename_action">OK</string>
    <!-- Buttons in Rename gesture dialog box -->
    <string name="cancel_action">Zrušiť</string>
    <!-- Message displayed when the user opens the gestures settings screen -->
    <string name="gestures_loading">Nahrávanie giest...</string>
    <!-- Message displayed when the user has no gestures -->
    <string name="gestures_empty">Žiadne gestá</string>
    <!-- Title of the screen used to view/manage gestures -->
    <string name="gestures_activity">Gestá</string>
    <!-- Noun, menu item used to rename a gesture -->
    <string name="gestures_rename">Premenovať</string>
    <!-- Noun, menu item used to remove a gesture -->
    <string name="gestures_delete">Zmazať</string>
    <!-- Message displayed when a gesture is successfully deleted -->
    <string name="gestures_delete_success">Gesto zmazané</string>
    <!-- Title of dialog box -->
    <string name="gestures_rename_title">Premenovať gesto</string>
    <!-- Label of gesture name field in Rename gesture dialog box -->
    <string name="gestures_rename_label">Názov gesta</string>
    <!-- Message, displayed when the sdcard cannot be found, 1st parameter is the name of the file that stores the gestures.  CHAR LIMIT=80 -->
    <string name="gestures_error_loading">Nemožno načítať %s. Uistite sa že máte dostatok voľného miesta.</string>
</resources>
